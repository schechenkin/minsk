{
    "configurations": [
        {
            "name": "Debug msc",
            "type": "dotnet",
            "request": "launch",
            "preLaunchTask": "build",
            "projectPath": "${workspaceFolder}/src/msc/msc.csproj",
            "launchConfigurationId": "TargetFramework=;msc"
        },
        {
            "name": "Debug a.out",
            "type": "cppdbg",
            "request": "launch",
            "args": [],
            "stopAtEntry": true,
            "externalConsole": false,
            "preLaunchTask": "Build executable",
            "cwd": "${workspaceFolder}/src/msc",
            "program": "${workspaceFolder}/src/msc/a.out",
            "MIMode": "gdb",
            "miDebuggerPath": "gdb",
            "setupCommands": [
              {
                "description": "Enable pretty-printing for gdb",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
              }
            ]
          }
    ]
}